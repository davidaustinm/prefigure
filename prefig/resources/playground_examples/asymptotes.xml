<diagram dimensions="(300,300)" margins="0">
  <definition>f(x)=(x^2-1)/(x^2-4)</definition>
  <coordinates bbox="(-5,-5,5,5)">
    <grid-axes/>
    <graph function="f"/>
    <line endpoints="((-2,-4),(-2,4))" 
          infinite="yes" stroke="red"/>
    <line endpoints="((2,4),(2,-4))"
          infinite="yes" stroke="red"/>
    <line endpoints="((4,1),(-4,1))"
          infinite="yes" stroke="green"/>
  </coordinates>
</diagram>
